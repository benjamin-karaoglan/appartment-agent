# =============================================================================
# Terraform GCS Backend Configuration
# =============================================================================
#
# Copy this file to backend.hcl and fill in your values:
#   cp backend.hcl.example backend.hcl
#
# Then initialize Terraform:
#   terraform init -backend-config=backend.hcl
#
# Bucket naming convention: <PROJECT_ID>-appart-tfstate
# =============================================================================

# GCS bucket for storing Terraform state
# Created by: ./scripts/setup-remote-state.sh <PROJECT_ID>
bucket = "YOUR_PROJECT_ID-appartagent-tfstate"

# Prefix (path) within the bucket for the state file
prefix = "terraform/state"
